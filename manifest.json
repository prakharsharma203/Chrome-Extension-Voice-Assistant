{
    "manifest_version": 3,
    "name": "being curious",
    "version": "1.0",
    "description": "A chrome extension for efficient browsing",
    "icons": {
        "16": "images/icon-16.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "content_scripts": [
        {
            "js": [
                "scripts/popup.js",
                "scripts/content.js"
            ],
            "css": [
                "scripts/content.css"
            ],
            "matches": [
                "*://*/*"
            ]
        }
    ]
}